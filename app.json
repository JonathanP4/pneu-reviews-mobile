{
	"expo": {
		"name": "pneu-reviews",
		"slug": "pneu-reviews",
		"version": "1.2.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "pneu-reviews",
		"userInterfaceStyle": "automatic",
		"backgroundColor": "#000000",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#000000"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/icon.png",
				"backgroundColor": "#000000"
			},
			"googleServicesFile": "./google-services.json",
			"package": "com.john404.pneureviews"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"expo-font",
			"@react-native-google-signin/google-signin",
			"@react-native-firebase/app",
			"@react-native-firebase/auth"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "e46d5068-4ac3-4c5f-b934-0e8b934caf25"
			}
		},
		"owner": "john404"
	}
}
